{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"lib/**"
			]
		},
		"lint": {
			"dependsOn": [
				"^lint"
			]
		},
		"format": {
			"dependsOn": [
				"^format"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}